{  
   "manifest_version":2,
   "name":"JS Kernel",
   "version":"0",
   "permissions":[  
      "activeTab",
      "webRequest",
      "webRequestBlocking",
      "downloads",
      "storage",
      "tabs",
      "http://*/*",
      "https://*/*",
      "webNavigation",
      "*://*/*"
   ],
   "content_scripts":[  
      {  
         "matches":[  
            "file:///*/*",
            "http://*/*",
            "https://*/*"
         ],
         "all_frames":true,
         "run_at":"document_start",
         "js":[  
            "policy.js",
            "load_policy.js",
            "jskernel.js"
         ]
      }
   ]
}
